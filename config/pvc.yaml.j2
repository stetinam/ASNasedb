kind: PersistentVolumeClaim
apiVersion: v1
metadata:
  name: asparaginase-seq-database
  namespace: {{ namespace }}
spec:
  accessModes:
    - ReadWriteMany
  resources:
    requests:
      storage: 2Gi
  storageClassName: nfs-csi
